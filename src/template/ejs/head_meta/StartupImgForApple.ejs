<% if(Array.isArray( startupImgForApple )){ %>
    <% for( const {
        href,
        type,
        crossorigin,
        sizes,
        media,
    } of
            startupImgForApple ){ %>
        <link
                rel = 'apple-touch-startup-image'
                type = '<%= type %>'
                sizes = '<%= sizes %>'
                media = '<%= media %>'
                href = '<%= href %>'
                crossorigin = '<%= crossorigin %>' />
    <% } %>
<% } %>
