{
  "display": "web-project-template for ts-node",
  "_version": "1.0.0",
  "_typescript_version": "4.9.3",
  "_ts-node_version": "10.9.1",
  "_description": "这是给基于ts-node运行TypeScript的Node一类的项目使用的基本通用的tsconfig.json。",
  "$schema": "https://json.schemastore.org/tsconfig",
  "docs": "https://www.typescriptlang.org/tsconfig",
  "_ts-node_schema": "https://unpkg.com/ts-node/tsconfig.schema.json",
  "_ts-node_docs": "https://typestrong.org/ts-node/docs",
  "_tsc_cli": "tsc -p ./ts-node-tsconfig.json --generateCpuProfile ./ts_compiled/ts-node/TSCOutputProfile.cpuprofile.json5",
  "extends": "./tsconfig.json",
  "include": [],
  "compilerOptions": {
    "declarationDir": "./ts_compiled/ts-node/",
    "outDir": "./ts_compiled/ts-node/",
    "generateTrace": "./ts_compiled/ts-node/",
    "tsBuildInfoFile": "./ts_compiled/ts-node/TSBuildInfo.tsbuildinfo.json5",
    "paths": {
      "tslib": [
        "./node_modules/tslib/"
      ]
    }
  },
  "ts-node": {
    "compiler": "typescript",
    "compilerHost": false,
    "emit": false,
    "esm": true,
    "experimentalReplAwait": true,
    "experimentalResolver": true,
    "experimentalSpecifierResolution": "node",
    "experimentalTsImportSpecifiers": true,
    "files": false,
    "logError": false,
    "preferTsExts": true,
    "pretty": true,
    "skipIgnore": false,
    "swc": false,
    "transpileOnly": false
  }
}
