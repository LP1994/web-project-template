{
  "display": "web-project-template for ts-node-esm",
  "_version": "1.0.0",
  "_typescript_version": "5.0.4",
  "_description": "这是给ts-node-esm使用的tsconfig.json。",
  "$schema": "https://json.schemastore.org/tsconfig",
  "docs": "https://www.typescriptlang.org/tsconfig",
  "_ts-node_schema": "https://unpkg.com/ts-node/tsconfig.schema.json",
  "_ts-node_docs": "https://typestrong.org/ts-node/docs",
  "_tsc_cli": "tsc -p ./ts-node_tsconfig.json --generateCpuProfile ./ts_compiled/TSCOutputProfile.cpuprofile.json5",
  "extends": "./tsconfig.json",
  "include": [
    "./src/**/*.ts",
    "./src/**/*.cts",
    "./src/**/*.mts",
    "./src/**/*.tsx",
    "./src/**/*.d.ts",
    "./src/**/*.d.cts",
    "./src/**/*.d.mts",
    "./src/**/*.ts.vue",
    "./src/**/*.cts.vue",
    "./src/**/*.mts.vue",
    "./src/**/*.tsx.vue",
    "./subsystems/**/*.ts",
    "./subsystems/**/*.cts",
    "./subsystems/**/*.mts",
    "./subsystems/**/*.tsx",
    "./subsystems/**/*.d.ts",
    "./subsystems/**/*.d.cts",
    "./subsystems/**/*.d.mts",
    "./subsystems/**/*.ts.vue",
    "./subsystems/**/*.cts.vue",
    "./subsystems/**/*.mts.vue",
    "./subsystems/**/*.tsx.vue"
  ],
  "exclude": [
    "./.git",
    "./.idea",
    "./assist_tools",
    "./backups",
    "./bats",
    "./configures",
    "./dist",
    "./log",
    "./node_modules",
    "./notes",
    "./read_me",
    "./simulation_servers",
    "./test",
    "./ts_compiled",
    "./webpack_location",
    "./webpack_records",
    "./bower_components",
    "./jspm_packages"
  ],
  "compilerOptions": {
    "moduleResolution": "node",
    "paths": {
      "tslib": [
        "./node_modules/tslib/"
      ]
    }
  }
}
