{
  "display": "web-project-template for ts-node-esm",
  "_version": "1.0.0",
  "_typescript_version": "5.0.2",
  "_description": "这是给ts-node-esm使用的tsconfig.json。",
  "$schema": "https://json.schemastore.org/tsconfig",
  "docs": "https://www.typescriptlang.org/tsconfig",
  "_ts-node_schema": "https://unpkg.com/ts-node/tsconfig.schema.json",
  "_ts-node_docs": "https://typestrong.org/ts-node/docs",
  "_tsc_cli": "tsc -p ./ts-node_tsconfig.json --generateCpuProfile ./ts_compiled/TSCOutputProfile.cpuprofile.json5",
  "extends": "./tsconfig.json",
  "include": [
    "./simulation_servers/**/*.ts",
    "./simulation_servers/**/*.cts",
    "./simulation_servers/**/*.mts",
    "./simulation_servers/**/*.tsx",
    "./simulation_servers/**/*.d.ts",
    "./simulation_servers/**/*.d.cts",
    "./simulation_servers/**/*.d.mts",
    "./simulation_servers/**/*.ts.vue",
    "./simulation_servers/**/*.cts.vue",
    "./simulation_servers/**/*.mts.vue",
    "./simulation_servers/**/*.tsx.vue",
    "./src/**/*.ts",
    "./src/**/*.cts",
    "./src/**/*.mts",
    "./src/**/*.tsx",
    "./src/**/*.d.ts",
    "./src/**/*.d.cts",
    "./src/**/*.d.mts",
    "./src/**/*.ts.vue",
    "./src/**/*.cts.vue",
    "./src/**/*.mts.vue",
    "./src/**/*.tsx.vue",
    "./subsystems/**/*.ts",
    "./subsystems/**/*.cts",
    "./subsystems/**/*.mts",
    "./subsystems/**/*.tsx",
    "./subsystems/**/*.d.ts",
    "./subsystems/**/*.d.cts",
    "./subsystems/**/*.d.mts",
    "./subsystems/**/*.ts.vue",
    "./subsystems/**/*.cts.vue",
    "./subsystems/**/*.mts.vue",
    "./subsystems/**/*.tsx.vue"
  ],
  "exclude": [
    "./.git",
    "./.idea",
    "./assist_tools",
    "./backups",
    "./bats",
    "./configures",
    "./dist",
    "./log",
    "./node_modules",
    "./notes",
    "./read_me",
    "./test",
    "./ts_compiled",
    "./webpack_location",
    "./webpack_records",
    "./bower_components",
    "./jspm_packages"
  ],
  "typeAcquisition": {
    "enable": true,
    "disableFilenameBasedTypeAcquisition": false
  },
  "ts-node": {
    "compiler": "typescript",
    "compilerHost": false,
    "emit": false,
    "esm": true,
    "experimentalReplAwait": true,
    "experimentalResolver": false,
    "experimentalTsImportSpecifiers": true,
    "files": false,
    "logError": false,
    "preferTsExts": true,
    "pretty": true,
    "skipIgnore": false,
    "swc": false,
    "transpileOnly": false
  },
  "compileOnSave": true,
  "compilerOptions": {
    "noEmitOnError": false,
    "allowJs": true,
    "checkJs": false,
    "noEmit": false,
    "emitDeclarationOnly": false,
    "allowImportingTsExtensions": true,
    "allowArbitraryExtensions": true,
    "verbatimModuleSyntax": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "alwaysStrict": true,
    "exactOptionalPropertyTypes": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noPropertyAccessFromIndexSignature": false,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "useUnknownInCatchVariables": true,
    "allowUmdGlobalAccess": true,
    "noResolve": false,
    "resolveJsonModule": true,
    "declaration": true,
    "declarationMap": true,
    "downlevelIteration": true,
    "emitBOM": false,
    "importHelpers": true,
    "inlineSourceMap": false,
    "inlineSources": false,
    "newLine": "crlf",
    "noEmitHelpers": false,
    "preserveConstEnums": true,
    "removeComments": false,
    "sourceMap": true,
    "stripInternal": false,
    "maxNodeModuleJsDepth": 0,
    "disableSizeLimit": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "preserveSymlinks": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "jsxFragmentFactory": "React.Fragment",
    "moduleDetection": "auto",
    "noLib": false,
    "useDefineForClassFields": true,
    "explainFiles": true,
    "extendedDiagnostics": false,
    "listEmittedFiles": true,
    "traceResolution": false,
    "composite": true,
    "disableReferencedProjectLoad": false,
    "disableSolutionSearching": false,
    "disableSourceOfProjectReferenceRedirect": false,
    "incremental": true,
    "noErrorTruncation": true,
    "preserveWatchOutput": true,
    "pretty": true,
    "skipLibCheck": true,
    "assumeChangesOnlyAffectDirectDependencies": true,
    "tsBuildInfoFile": "./ts_compiled/TSBuildInfo.tsbuildinfo.json5",
    "target": "es2022",
    "module": "es2022",
    "moduleResolution": "node",
    "declarationDir": "./ts_compiled/",
    "outDir": "./ts_compiled/",
    "generateTrace": "./ts_compiled/",
    "baseUrl": "./",
    "rootDir": ".",
    "types": [],
    "typeRoots": [
      "./node_modules/@types"
    ],
    "moduleSuffixes": [
      ""
    ],
    "paths": {
      "tslib": [
        "./node_modules/tslib/"
      ]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "es2015",
      "es2015.collection",
      "es2015.core",
      "es2015.generator",
      "es2015.iterable",
      "es2015.promise",
      "es2015.proxy",
      "es2015.reflect",
      "es2015.symbol",
      "es2015.symbol.wellknown",
      "es2016",
      "es2016.array.include",
      "es2017",
      "es2017.intl",
      "es2017.object",
      "es2017.sharedmemory",
      "es2017.string",
      "es2017.typedarrays",
      "es2018",
      "es2018.asyncgenerator",
      "es2018.asynciterable",
      "es2018.intl",
      "es2018.promise",
      "es2018.regexp",
      "es2019",
      "es2019.array",
      "es2019.intl",
      "es2019.object",
      "es2019.string",
      "es2019.symbol",
      "es2020",
      "es2020.bigint",
      "es2020.date",
      "es2020.intl",
      "es2020.number",
      "es2020.promise",
      "es2020.sharedmemory",
      "es2020.string",
      "es2020.symbol.wellknown",
      "es2021",
      "es2021.intl",
      "es2021.promise",
      "es2021.string",
      "es2021.weakref",
      "es2022",
      "es2022.array",
      "es2022.error",
      "es2022.intl",
      "es2022.object",
      "es2022.sharedmemory",
      "es2022.string",
      "es5",
      "es6",
      "esnext",
      "esnext.intl",
      "esnext.promise",
      "esnext.string",
      "esnext.weakref",
      "scripthost",
      "webworker",
      "webworker.importscripts",
      "webworker.iterable"
    ]
  }
}
