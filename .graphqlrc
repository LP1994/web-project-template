{
  "projects": {
    "web-project-template": {
      "name": "web-project-template",
      "schema": "./src/graphQL/GraphQL.Schema.json",
      "documents": "./src/graphQL/doc/**/*.{graphql,graphqls,gql}",
      "include": [
        "./src/**/*.{graphql,graphqls,gql}",
        "./webpack_location/**/*.{graphql,graphqls,gql}"
      ],
      "exclude": [
        "./src/graphQL/doc/**/*.{graphql,graphqls,gql}"
      ],
      "extensions": {
        "endpoints": {
          "deno:http://127.0.0.1:9000": {
            "url": "http://127.0.0.1:9000/graphql",
            "introspect": false,
            "headers": {
              "Accept": "*/*",
              "Accept-Encoding": "gzip",
              "Cache-Control": "no-cache",
              "Connection": "keep-alive",
              "Content-Type": "application/json",
              "Pragma": "no-cache",
              "user-agent": "GraphQL plugin from WebStorm.",
              "Access-Control-Request-Headers": "Authorization, Accept, Content-Type, Content-Language, Accept-Language",
              "Access-Control-Request-Method": "GET, HEAD, POST, PUT, DELETE, CONNECT, OPTIONS, TRACE, PATCH"
            }
          }
        }
      }
    },
    "simulation_servers_deno": {
      "name": "simulation_servers_deno",
      "schema": "./src/graphQL/GraphQL.Schema.json",
      "documents": "./src/graphQL/doc/**/*.{graphql,graphqls,gql}",
      "include": [
        "./simulation_servers/deno/src/**/*.{graphql,graphqls,gql}"
      ],
      "exclude": [
        "./simulation_servers/deno/src/**/*.type.{graphql,graphqls,gql}"
      ],
      "extensions": {
        "endpoints": {
          "deno:http://127.0.0.1:9000": {
            "url": "http://127.0.0.1:9000/graphql",
            "introspect": false,
            "headers": {
              "Accept": "*/*",
              "Accept-Encoding": "gzip",
              "Cache-Control": "no-cache",
              "Connection": "keep-alive",
              "Content-Type": "application/json",
              "Pragma": "no-cache",
              "user-agent": "GraphQL plugin from WebStorm.",
              "Access-Control-Request-Headers": "Authorization, Accept, Content-Type, Content-Language, Accept-Language",
              "Access-Control-Request-Method": "GET, HEAD, POST, PUT, DELETE, CONNECT, OPTIONS, TRACE, PATCH"
            }
          }
        }
      }
    }
  }
}
