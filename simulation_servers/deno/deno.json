{
  "tasks": {
    "start": "deno run -A --config=./deno.json --check --v8-flags=--max-old-space-size=1024000 --reload --watch-hmr --env-file=./.env.deno ./src/App.mts --color=16m"
  },
  "nodeModulesDir": "none",
  "lock": "./deno.lock",
  "exclude": [],
  "unstable": [
    "webgpu",
    "broadcast-channel",
    "worker-options",
    "cron",
    "kv",
    "net"
  ],
  "lint": {
    "include": [
      "src/"
    ],
    "exclude": [
      "src/custom_declare_types/",
      "src/**/type_doc/doc/",
      "src/**/third_party_modules/",
      "src/**/graphql_schema_definition_to_ts_type_definition/",
      "test/"
    ],
    "rules": {
      "tags": [
        "recommended"
      ],
      "include": [
        "eqeqeq",
        "explicit-function-return-type",
        "guard-for-in",
        "no-const-assign",
        "no-eval",
        "no-self-compare",
        "no-sparse-arrays",
        "no-throw-literal",
        "no-undef",
        "triple-slash-reference"
      ],
      "exclude": [
        "no-explicit-any",
        "ban-ts-comment",
        "prefer-const",
        "no-case-declarations",
        "ban-untagged-todo",
        "camelcase",
        "default-param-last",
        "explicit-module-boundary-types",
        "fresh-handler-export",
        "fresh-server-event-handlers",
        "no-await-in-loop",
        "no-console",
        "no-external-import",
        "no-implicit-declare-namespace-export",
        "no-inferrable-types",
        "no-non-null-asserted-optional-chain",
        "no-non-null-assertion",
        "no-top-level-await",
        "prefer-ascii",
        "prefer-primordials",
        "single-var-declarator",
        "verbatim-module-syntax",
        "no-sync-fn-in-async-fn"
      ]
    }
  },
  "compilerOptions": {
    "checkJs": false,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "jsxFragmentFactory": "React.Fragment",
    "noErrorTruncation": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noUncheckedIndexedAccess": true,
    "strict": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "strictNullChecks": true,

    "useUnknownInCatchVariables": true,

    "exactOptionalPropertyTypes": true,
    "noPropertyAccessFromIndexSignature": false,

    "verbatimModuleSyntax": true,
    "emitDecoratorMetadata": false,
    "experimentalDecorators": false,

    "lib": [
      "deno.broadcast_channel",
      "deno.cache",
      "deno.canvas",
      "deno.console",
      "deno.crypto",
      "deno.fetch",
      "deno.net",
      "deno.ns",
      "deno.shared_globals",
      "deno.unstable",
      "deno.url",
      "deno.web",
      "deno.webgpu",
      "deno.websocket",
      "deno.webstorage",
      "deno.window",
      "decorators",
      "decorators.legacy",
      "es2015",
      "es2015.collection",
      "es2015.core",
      "es2015.generator",
      "es2015.iterable",
      "es2015.promise",
      "es2015.proxy",
      "es2015.reflect",
      "es2015.symbol",
      "es2015.symbol.wellknown",
      "es2016",
      "es2016.array.include",
      "es2016.intl",
      "es2017",
      "es2017.date",
      "es2017.intl",
      "es2017.object",
      "es2017.sharedmemory",
      "es2017.string",
      "es2017.typedarrays",
      "es2018",
      "es2018.asyncgenerator",
      "es2018.asynciterable",
      "es2018.intl",
      "es2018.promise",
      "es2018.regexp",
      "es2019",
      "es2019.array",
      "es2019.intl",
      "es2019.object",
      "es2019.string",
      "es2019.symbol",
      "es2020",
      "es2020.bigint",
      "es2020.date",
      "es2020.intl",
      "es2020.number",
      "es2020.promise",
      "es2020.sharedmemory",
      "es2020.string",
      "es2020.symbol.wellknown",
      "es2021",
      "es2021.intl",
      "es2021.promise",
      "es2021.string",
      "es2021.weakref",
      "es2022",
      "es2022.array",
      "es2022.error",
      "es2022.intl",
      "es2022.object",
      "es2022.regexp",
      "es2022.sharedmemory",
      "es2022.string",
      "es2023",
      "es2023.array",
      "es2023.collection",
      "es2023.intl",
      "es5",
      "es6",
      "esnext",
      "esnext.array",
      "esnext.collection",
      "esnext.decorators",
      "esnext.disposable",
      "esnext.intl",
      "esnext.object",
      "esnext.promise",
      "esnext.regexp",
      "esnext.string"
    ]
  },
  "scopes": {
    "https://esm.sh/": {
      "https://esm.sh/v135/@types/graphql@14.5.0/index.d.ts": "https://esm.sh/graphql/index.d.ts",
      "https://esm.sh/v135/graphql@16.8.1/denonext/graphql.mjs": "https://esm.sh/v135/graphql@16.9.0/denonext/graphql.mjs"
    }
  },
  "imports": {
    "deno_std/": "https://deno.land/std/",
    "deno_std_encoding/": "https://deno.land/std/encoding/",
    "deno_std_io/": "https://deno.land/std/io/",
    "deno_std_media_types": "https://deno.land/std/media_types/mod.ts",
    "deno_std_path": "https://deno.land/std/path/mod.ts",

    "deno_x/": "https://deno.land/x/",
    "deno_x_corejs": "https://deno.land/x/corejs/index.js",
    "deno_x_ejs": "https://deno.land/x/dejs/mod.ts",
    "deno_x_mongo": "https://deno.land/x/mongo/mod.ts",

    "esm_sh/": "https://esm.sh/",
    "esm_sh_graphql": "https://esm.sh/graphql",

    "preact/": "https://cdn.skypack.dev/preact/",
    "preact_jsx-runtime": "https://cdn.skypack.dev/preact/jsx-runtime?dts",
    "preact_jsx-dev-runtime": "https://cdn.skypack.dev/preact/jsx-dev-runtime?dts",

    "deno_src/": "./src/",
    "configures/": "./src/configures/",
    "route_map_config/": "./src/configures/route_map_config/",
    "database/": "./src/database/",
    "deno_db/": "./src/database/deno_db/",
    "firebase/": "./src/database/firebase/",
    "graphql/": "./src/database/graphql/",
    "mongo/": "./src/database/mongo/",
    "mysql/": "./src/database/mysql/",
    "postgres/": "./src/database/postgres/",
    "sqlite/": "./src/database/sqlite/",
    "supabase/": "./src/database/supabase/",
    "public/": "./src/public/",
    "routers/": "./src/routers/",
    "servers/": "./src/servers/",
    "services/": "./src/services/",
    "ssr/": "./src/ssr/",
    "template/": "./src/template/",
    "ejs/": "./src/template/ejs/",
    "handlebars/": "./src/template/handlebars/",
    "html/": "./src/template/html/",
    "markdown/": "./src/template/markdown/",
    "mustache/": "./src/template/mustache/",
    "pug_jade/": "./src/template/pug_jade/",
    "tools/": "./src/tools/",
    "third_party_modules/": "./src/tools/third_party_modules/",
    "universal_tool_for_deno/": "./src/tools/universal_tool_for_deno/",
    "static/": "./static/",
    "test/": "./test/",
    "upload/": "./upload/",
    "web/": "./web/",

    "GSD2TSTD": "./src/graphql_schema_definition_to_ts_type_definition/GSD2TSTD.esm.mts"
  }
}
