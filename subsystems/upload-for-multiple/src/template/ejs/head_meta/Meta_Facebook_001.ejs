<% if(facebook !== null && facebook !== undefined){ %>
    <!--
    使用Facebook的分析工具，必须提供唯一的ID号码关联你的账户，分析要用的网站应用的Facebook应用编号。
    值如：87741124305。
    -->
    <% if(facebook.appID !== null && facebook.appID !== undefined){ %>
        <meta property = 'fb:app_id'
              content = '<%= facebook.appID %>' />
    <% } %>

    <!--值如：100001422224225-->
    <% if(facebook.admins !== null && facebook.admins !== undefined){ %>
        <meta property = 'fb:admins'
              content = '<%= facebook.admins %>' />
    <% } %>
    <!--值如：241333394220-->
    <% if(facebook.pageID !== null && facebook.pageID !== undefined){ %>
        <meta property = 'fb:page_id'
              content = '<%= facebook.pageID %>' />
    <% } %>
<% } %>
