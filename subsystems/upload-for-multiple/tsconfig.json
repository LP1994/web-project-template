  "_tsc_cli": "tsc -p ./tsconfig.json",
  "extends": "./configures/tsconfig/tsconfig.web.json5",
  "include": [
    "./**/*.ts",
    "./**/*.cts",
    "./**/*.mts",
    "./**/*.tsx",
    "./**/*.d.ts",
    "./**/*.d.cts",
    "./**/*.d.mts",
    "./**/*.ts.vue",
    "./**/*.cts.vue",
    "./**/*.mts.vue",
