{
  "display": "web-project-template",
  "_version": "1.0.0",
  "_typescript_version": "4.7.3",
  "_description": "这是给web-project-template项目使用的tsconfig.json。",
  "$schema": "https://json.schemastore.org/tsconfig",
  "docs": "https://www.typescriptlang.org/tsconfig",
  "_tsc_cli": "tsc -p tsconfig.json --generateCpuProfile ./ts_compiled/TSCOutputProfile.cpuprofile.json5",
  "extends": "./configures/tsconfig/web.tsconfig.json5",
  "include": [
    "./src/**/*.ts",
    "./src/**/*.cts",
    "./src/**/*.mts",
    "./src/**/*.tsx",
    "./src/**/*.d.ts",
    "./src/**/*.d.cts",
    "./src/**/*.d.mts",
    "./src/**/*.ts.vue",
    "./src/**/*.cts.vue",
    "./src/**/*.mts.vue",
    "./src/**/*.tsx.vue",
    "./subsystems/**/*.ts",
    "./subsystems/**/*.cts",
    "./subsystems/**/*.mts",
    "./subsystems/**/*.tsx",
    "./subsystems/**/*.d.ts",
    "./subsystems/**/*.d.cts",
    "./subsystems/**/*.d.mts",
    "./subsystems/**/*.ts.vue",
    "./subsystems/**/*.cts.vue",
    "./subsystems/**/*.mts.vue",
    "./subsystems/**/*.tsx.vue"
  ],
  "exclude": [
    "./dist",
    "./node_modules",
    "./bower_components",
    "./jspm_packages"
  ],
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "ElementUI_V2_CSS": [
        "./node_modules/element-ui/lib/theme-chalk/index.css"
      ],
      "ElementUI_CSS": [
        "./node_modules/element-plus/dist/index.css"
      ],
      "SwiperCSS": [
        "./node_modules/swiper/swiper-bundle.min.css"
      ],
      "tslib": [
        "./node_modules/tslib/"
      ]
    },
    "rootDir": ".",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": [],
    "declarationDir": "./ts_compiled/",
    "outDir": "./ts_compiled/",
    "tsBuildInfoFile": "./ts_compiled/TSBuildInfo.tsbuildinfo.json5"
  }
}
